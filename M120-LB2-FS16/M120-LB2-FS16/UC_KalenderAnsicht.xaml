             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:M120_LB2_FS16"
             mc:Ignorable="d" d:DesignWidth="300" Width="800" Height="1500" Margin="20">
    <Grid x:Name="kalender" Width="750" ShowGridLines="True" VerticalAlignment="Top" HorizontalAlignment="Left" Height="1500">
