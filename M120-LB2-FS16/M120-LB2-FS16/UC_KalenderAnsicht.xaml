            <ColumnDefinition Width="50"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
            <ColumnDefinition Width="100"/>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="1440"/>
        </Grid.RowDefinitions>
        <Label x:Name="montag_datum" Grid.Row="0" Grid.Column="1" Content="Montag"/>
        <Label x:Name="dienstag_datum" Grid.Row="0" Grid.Column="2" Content="Dienstag"/>
        <Label x:Name="mittwoch_datum" Grid.Row="0" Grid.Column="3" Content="Mittwoch"/>
        <Label x:Name="donnerstag_datum" Grid.Row="0" Grid.Column="4" Content="Donnerstag"/>
        <Label x:Name="freitag_datum" Grid.Row="0" Grid.Column="5" Content="Freitag"/>
        <Label x:Name="samstag_datum" Grid.Row="0" Grid.Column="6" Content="Samstag"/>
        <Label x:Name="sonntag_datum" Grid.Row="0" Grid.Column="7" Content="Sonntag"/>
        <Grid x:Name="montag_content" Height="1440" Width="100"     Margin="0"      Grid.Row="1"    Grid.Column="1">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Grid x:Name="dienstag_content" Height="1440" Width="100"   Margin="0"      Grid.Row="1"    Grid.Column="2">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Grid x:Name="mittwoch_content" Height="1440" Width="100"   Margin="0"      Grid.Row="1"    Grid.Column="3">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Grid x:Name="donnerstag_content" Height="1440" Width="100" Margin="0"      Grid.Row="1"    Grid.Column="4">
