                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="50"/>
                <ColumnDefinition Width="50"/>
            </Grid.ColumnDefinitions>
        </Grid>
        <Grid x:Name="sonntag_content" Height="1440" Width="100"    Margin="10,1,-10,0"      Grid.Row="1"    Grid.Column="7">
